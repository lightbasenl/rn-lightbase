{
  "name": "example",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@capsizecss/metrics": "1.1.1",
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo-google-fonts/roboto": "^0.2.3",
    "@lightbase/rn-design-system": "*",
    "@lightbase/native-assets": "*",
    "expo": "~48.0.18",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-reanimated": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/config-plugins": "^6.0.1",
    "@lightbase/eslint-config-rn": "*",
    "@types/react": "~18.0.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "typescript": "^4.9.4"
  },
  "private": true,
  "eslintConfig": {
    "extends": "@lightbase/eslint-config-rn",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ]
