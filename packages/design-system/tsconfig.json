{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "*" : ["src/*"],
    },
    "declaration": true,
    "preserveSymlinks": true,
    "target": "es6",
    "module": "ESNext",
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom", "es2017"],
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
}
